/* Spes NFC */
&qupv3_se4_i2c {
		status = "ok";
	qcom,clk-freq-out = <400000>;
	#address-cells = <1>;
	#size-cells = <0>;


      st_st21nfc@8 {
        compatible = "st,st21nfc";
        reg = <0x08>;
        st,irq-gpio = <&tlmm 70 0>;
        st,reset-gpio = <&tlmm 69 0>;
        st,clkreq-gpio = <&tlmm 71 0>;
        interrupt-parent = <&tlmm>;
        interrupts = <70 0>;
        interrupt-names = "nfc_irq";
        clock-names = "nfc_ref_clk";
        st,clk_pinctrl;
      };
};

&tlmm {
  rt1711h_int_n:
    rt1711h_int_n {
		mux {
			pins = "gpio93";
			function = "gpio";
		};

		config {
			pins = "gpio93";
			drive-strength = <2>;
			bias-pull-up;
		};
	};
};	

&qupv3_se1_i2c {
  rt1711h@4e {
			compatible = "richtek,rt1711h";
			reg = <0x4e>;
			status = "okay";
			tcpc-dual,supported_modes = <0>;
			rt-tcpc,name = "type_c_port0";
			rt-tcpc,role_def = <4>;
			rt-tcpc,rp_level = <0>;
			rt-tcpc,vconn_supply = <1>;
			rt1711pd,intr_gpio = <&tlmm 93 0>;
			rt1711pd,intr_gpio_num = <93>;
			pinctrl-names = "default";
			pinctrl-0 = <&rt1711h_int_n>;

			pd-data {
				pd,vid = <10703>;
				pd,pid = <5905>;
				pd,source-cap-ext = [cf 29 11 17 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 02 00];
				pd,mfrs = "RichtekTCPC";
				pd,charging_policy = <49>;
				pd,source-pdo-size = <1>;
				pd,source-pdo-data = <102450>;
				pd,sink-pdo-size = <1>;
				pd,sink-pdo-data = <102600>;
				pd,id-vdo-size = <6>;
				pd,id-vdo-data = <3510643151 0 386990080 1627389952 0 1090519040>;
				bat,nr = <1>;
				pd,country_nr = <0>;

				bat-info0 {
					bat,vid = <10703>;
					bat,pid = <5905>;
					bat,mfrs = "bat1";
					bat,design_cap = <3000>;
				};
			};

			dpm_caps {
				local_dr_power;
				local_dr_data;
				local_usb_comm;
				local_no_suspend;
				local_vconn_supply;
				attemp_enter_dp_mode;
				attemp_discover_cable;
				attemp_discover_id;
				pr_check = <0>;
				dr_check = <0>;
			};

			displayport {
				1st_connection = "dfp_d";
				2nd_connection = "dfp_d";
				signal,dp_v13;
				typec,receptacle;

				ufp_d {
				};

				dfp_d {
					pin_assignment,mode_c;
					pin_assignment,mode_d;
					pin_assignment,mode_e;
				};
			};
		};
};

