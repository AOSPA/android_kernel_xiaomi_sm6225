&tlmm {
/* BQ-CHARGER */
	bq2589x_int_default:bq2589x_int_default {

				mux {
					pins = "gpio105";
					function = "gpio";
				};

				config {
					pins = "gpio105";
					drive-strength = <2>;
					bias-pull-up;
					input-enable;
				};
			};

	bq2589x_usb_switch_default:bq2589x_usb_switch_default {

				mux {
					pins = "gpio31";
					function = "gpio";
				};

				config {
					pins = "gpio31";
					drive-strength = <2>;
					bias-pull-down;
					output-low;
				};
			};
};

